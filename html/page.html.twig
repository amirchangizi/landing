{% extends ":"~template~":base.html.twig" %}
{% block content %}
  <div id="app">
    <div class="frame inner-frame">
      <!--    هدر    -->
      <header id="header" class="mb-30" style="position: relative;
    overflow: hidden;">
        <picture>
          <img src="assets/images/landing-header-min.png" alt="" style="width:auto;max-width: 100%">
        </picture>
      </header>

      <!--    دکمه    -->
      <!--        <section class="mb-30">-->
      <!--            <button type="submit" class="btn-landing btn-header scroll-to" id="" >ثبت نام رژیم</button>-->
      <!--        </section>-->


      <section class="container mb-20" v-if="userInfo.utm_campaign=='null' || userInfo.utm_campaign=='null1'">

        <div class="countdown-container">
          <h3 class="h5 text-dark">این فرصت هیچ وقت تکرار نمی‌شه!</h3>
          <div class="timer-container">
            <h1 class="alt-1">{{getLocalDateTime}}T23:59:59+0000</h1>
          </div>
          <p class="h6 text-dark" style="margin-top: 30px;">مانده تا پایان تخفیف</p>
        </div>

      </section>


      <!--            دکمه    -->
      <section class="mb-30" v-if="false">
        <button type="submit" class="btn-landing btn-header scroll-to" id="" >ثبت نام رژیم</button>
      </section>


      <!--    متن ویدئو    -->
      <section class="container mb-30">
        <header class="header-title">
          <h2>عید، لباس تن مانکن رو بپوش!</h2>
        </header>
        <p class="text-justify mb-2" style="color: #000">اگه می‌خوای عید، لباس‌ تن مانکن‌ رو بپوشی و تو مهمونی‌ها همه از لاغر شدنت سوپرایز شن، الان وقتشه! فقط 3 ماه تا عید مونده، تو این فرصت می‌تونی با موفق‌ترین روش لاغری برنافیت رژیم بگیری و <span class="color-red text-bold">12 کیلو</span> وزن کم کنی و <span class="color-red text-bold">4 سایز</span> لاغر شی.</p>
        <p class="text-justify" style="color: #000">می‌تونی عید جذاب‌ترین اندام رو داشته باشی و قشنگ‌ترین لباس‌ها رو بپوشی، انتخاب با خودته!<p>
      </section>

      <!--    ویدئو    -->
      <section class="container mb-30">
        <div class="video-container video-container-pro" style="margin: 0 auto 25px;">
          <div class="video_holder">
                <span class="has-icon">
                    <video poster="assets/images/cover1.jpg" ref="video-1" preload="none">
                        <source src="assets/video/marathon-intro.mp4" type="video/mp4">
                        Your browser does not support HTML5 video.
                    </video>
                     <i id="ic-play1" class="ic-play video-record" @click="play(1)"></i>
                    <small>
                        <strong>پخش ویدئو</strong>
                     </small>
                </span>
          </div>
          <div class="panel">
                <span>
                 <strong>لاغری گروهی، جدیدترین رژیم دکتر کرمانی</strong>
                </span>
          </div>
        </div>
      </section>

      <!--    محاسبه bmi    -->
      <section class="container mb-30 bmi-register btn-hide" id="register">


        <div class="register-wrapper panel">
          <div class="register">
            <img src="assets/images/bmi-header-icon.png" class="register-header" alt="yalda">
            <div v-if="step==1">
              <h4 class="title my-0" v-if="childStep==1">برای ثبت نام و دریافت رژیم</h4>
              <strong class="my-2 mb-3" style="display: block" v-if="childStep==1">مشخصاتت رو وارد کن</strong>
              <h4 class="title px-4" v-if="childStep==2 && step !=2">گزارش میزان اضافه وزن شما</h4>
            </div>
            <span v-if="step==2 || step==3" class="section-btn" title="محاسبه مجدد">
                         <button @click="backButtonRegister" v-if="childStep !=1">
                            <i class="ic-arrow-right"></i>
                         </button>
                    </span>
            <div v-if="step==2">
              <h4 class="title my-0">برای ثبت نام و دریافت رژیم</h4>
              <strong class="my-2 mb-3" style="display: block">مشخصاتت رو وارد کن</strong>
            </div>

            <div v-if="step==3">
              <h4 class="title my-0">ثبت نام و دریافت رژیم</h4>
            </div>

            <span v-if="step==1 && childStep==2" class="section-btn" title="محاسبه مجدد">
                        <button @click="reloadBmi">
                            <i class="ic-arrow-right"></i>
                        </button>
                    </span>

            <!--                    <div v-if="step==3">-->
            <!--                        <h4 class="title" style="margin-bottom: 0">با بروزترین متد لاغری</h4>-->
            <!--                        <strong style="margin: 0 0 15px 15px;display: block">در {{result.months}} به وزن دلخواهت می‌رسی</strong>-->
            <!--                    </div>-->

            <div class="item-header">
                    <span v-for="n in 3"
                          :class="{active: n == step,elapsed:n< step}"
                    >{{ n }}</span>
            </div>
          </div>

          <transition name="fade" mode="out-in" tag="div">


            <!-- STEP 1 -->
            <div class="inner" v-if="step==1" :key="step">
              <transition name="fade" mode="out-in" tag="div">
                <div class="inner" v-if="step==1" :key="step">
                  <transition name="fade" mode="out-in" tag="div">
                    <div class="inner" v-if="childStep==1" :key="childStep">
                      <form @submit.prevent="calculate">
                        <div class="input-group">
                          <!--                                <span class="label">جنسیت:</span>-->
                          <div id="container-toggle">
                            <div class="inner-container-toggle">
                              <div class="toggle">
                                <input type="radio" id="inp1" name="gender" value="female" v-model="form.gender">
                                <label for="inp1" >خانم هستم</label>
                                <!--                                            <span class="icon-gender"></span>-->
                              </div>
                              <div class="toggle">
                                <input type="radio" id="inp2" name="gender" value="male" v-model="form.gender">
                                <label for="inp2" >آقا هستم</label>
                                <!--                                            <span class="icon-gender"></span>-->
                              </div>
                            </div>
                          </div>
                        </div>
                        <c-input
                                v-model.number="form.weight"
                                label="وزن (کیلوگرم)"
                                type="tel"
                                numeric
                                required
                                tips="20 تا 200"
                                max="10"
                                @validate="validateInput('weight')"
                        ></c-input>
                        <p v-if="formInputList.weight.hasError" class="small" style="color: red !important;">
                          وزن باید بین 20 تا 200 کیلو باشد
                        </p>
                        <c-input
                                v-model.number="form.height"
                                label="قد (سانتی متر)"
                                type="tel"
                                numeric
                                required
                                tips="100 تا 230"
                                @validate="validateInput('height')"
                        ></c-input>
                        <p v-if="formInputList.height.hasError" class="small" style="color: red !important;">
                          قد باید بین 100 تا 230 باشد
                        </p>
                        <c-input
                                v-model.number="form.age"
                                label="سن (سال)"
                                type="tel"
                                numeric
                                required
                                tips="12 تا 70"
                                @validate="validateInput('age')"
                        ></c-input>
                        <p v-if="formInputList.age.hasError" class="small" style="color: red !important;">
                          سن باید بین 12 تا 70 باشد
                        </p>
                        <!--                        <c-input-->
                        <!--                                v-model.number="form.wrist"-->
                        <!--                                label="دور مچ دست (سانتی متر)"-->
                        <!--                                type="tel"-->
                        <!--                                numeric-->
                        <!--                                required-->
                        <!--                                tips="5 تا 30"-->
                        <!--                                @validate="validateInput('wrist')"-->
                        <!--                        ></c-input>-->
                        <!--                        <p v-if="formInputList.wrist.hasError" class="small" style="color: red !important;">-->
                        <!--                            دور مچ دست باید بین 5 تا 30 باشد-->
                        <!--                        </p>-->
                        <button type="submit" class="btn-landing landing-getbmi" id="tor-info" :disabled="!validRegForm">تایید و مرحله
                          بعد
                          <i class="ic-arrow-right"></i>
                        </button>
                      </form>
                    </div>
                    <div class="inner" v-if="childStep==2" :key="childStep">
                      <div id="step2bmi-result">
                        <div class="report_bmi">
                          <div class="box">
                            <div class="step2bmi-result-item">
                              <div class="content">
                                <p>میزان {{result.dietType}}</p>
                                <div class="result-container">
                                  <span class="text-danger font-weight-bold h4">{{result.weight}}</span>
                                  <span class="result-unit">کیلوگرم</span>
                                </div>
                              </div>
                              <div class="icon-content">
                                <img style="transform: scaleX(-1);" src="assets/images/obesity.png" alt="">
                              </div>
                            </div>
                            <div class="step2bmi-result-item">
                              <div class="content">
                                <p>وزن ایده‌آل</p>
                                <div class="result-container">
                                  <span class="text-danger font-weight-bold h4">{{result.normalWeight}}</span>
                                  <span class="result-unit">کیلوگرم</span>
                                </div>
                              </div>
                              <div class="icon-content">
                                <img style="transform: scaleX(-1);" src="assets/images/weight-scale.png" alt="">
                              </div>
                            </div>
                            <div class="step2bmi-result-item">
                              <div class="content">
                                <p>زمان رسیدن به وزن ایده‌آل</p>
                                <div class="result-container">
                                  <span class="text-danger font-weight-bold h4">{{result.months}}</span>
                                  <span class="result-unit" v-if="result.months != '-'">ماه</span>
                                </div>
                              </div>
                              <div class="icon-content">
                                <img style="transform: scaleX(-1);" src="assets/images/clock.png" alt="">
                              </div>
                            </div>
                          </div>
                          <div class="chart">
                            <div class="chart_svg">
                              <img :src="result.chart.img" alt="">
                              <img :src="result.chart.holder" alt="">
                            </div>
                            <h3>{{result.chart.title}}</h3>
                          </div>
                        </div>

                        <div class="main_list mt-20 mb-20">
                          <ul class="sans">
                            <li class="mb-4">تا کنون <span class="mark">254 هزار نفر</span> از هم‌سن و سال‌های تو با رژیم برنافیت بدون بازگشت وزن به تناسب اندام رسیدن.</li>
                            <li>بیش از <span class="mark">90 درصد</span> از کسایی که با برنافیت رژیم گرفتن، از رژیم‌شون رضایت کامل داشتن.</li>
                          </ul>
                        </div>
                      </div>
                      <button @click="step=2; childStep = 1" class="btn-landing landing-getreport">ثبت‌نام رژیم<span v-if="loading" class="spinner-border text-light spinner-border-sm" role="status" aria-hidden="true" style="display: inline-block;margin-right: 5px;"></span>
                        <i v-else class="ic-arrow-right"></i>
                      </button>
                    </div>
                  </transition>
                </div>
              </transition>
            </div>

            <div class="inner" v-if="step==2" :key="step">
              <transition name="fade" mode="out-in" tag="div">
                <div class="inner" v-if="childStep==1" :key="childStep">
                  <form @submit.prevent="register(2,'bmi')">
                    <c-input label="نام" v-model="form.name" type="text" required class="person"></c-input>
                    <p v-if="invalidName" class="small" style="color: red !important;"> لطفا نام خودرا به درستی وارد کنید </p>
                    <c-input label="نام خانوادگی" v-model="form.family" type="text" required class="person"></c-input>
                    <p v-if="invalidFamily" class="small" style="color: red !important;"> لطفا نام خانوادگی خودرا به درستی وارد کنید </p>
                    <div class="tel-input-container mb-10" v-if="submitStatus == 'none'">
                      <select class="input_country_tel" v-model="countryListSelectId">
                        <option v-for="(item, index) in countryListCodeData" :key="index" :value="item.code" v-if="item.continent !== 'Africa'" :title="item.code.toLowerCase()">
                          {{item.code + " (+"+item.mobileCode+")"}}
                        </option>
                      </select>
                      <c-input v-model="form.phone" ref="tel-2" label="موبایل" type="tel" numeric required :max=validationPhoneCharacter style="width: 83%;margin: 0"></c-input>
                    </div>
                    <!--                                <p v-if="response.msg" v-html="response.msg" class="text-success">{{ response.msg }}</p>-->
                    <p v-if="validationPhone" class="small" style="color: #000 !important; z-index: 1">لطفا شماره موبایل را بصورت صحیح وارد کنید</p>
                    <button @click="" type="submit" class="btn-landing btn-register-user mt-auto landing-register" :disabled="(invalidName || invalidFamily || loading)" id="tor-register">مرحله بعد<span v-if="loading" class="spinner-border text-light spinner-border-sm" role="status" aria-hidden="true" style="display: inline-block;margin-right: 5px;"></span>
                      <i v-else class="ic-arrow-right"></i>
                    </button>
                  </form>
                </div>
                <div class="inner" v-if="childStep==2" :key="childStep">
                  <form @submit.prevent="verify">
                    <strong>کد فعالسازی به شماره موبایل زیر ارسال شد</strong>
                    <strong class="color-red mb-15">{{form.phone}}</strong>
                    <c-count :time="prettyTime" style="width: 70%;margin: 0 auto;" label="کد فعالسازی را وارد کنید" v-model="form.verify" type="num" required class="person" max="4"></c-count>
                    <c-input v-if="false" style="width: 70%;margin: auto;" label="کد فعالسازی را وارد کنید" v-model="form.verify" type="num" required class="person" max="4"></c-input>
                    <p v-if="form.isVerify" class="small mb-0 mt-15" style="color: red !important;">کد فعال سازی اشتباه است</p>
                    <!--                                    <div v-if="!form.registerCode">-->
                    <!--                                        <timer :time="prettyTime" class="mt-10"></timer>-->
                    <!--                                    </div>-->
                    <p class="mb-0 mt-3">درصورت عدم دریافت پیامک کد فعالسازی بعد از<br> پایان زمان انتظار دکمه زیر را لمس کنید</p>
                    <p :class="form.registerCode ? 'registerWCode mt-10' : 'deactive mt-10'" @click="verifyWCode"><u>کد فعالسازی دریافت نکردم</u></p>
                    <button @click="" type="submit" class="btn-landing btn-register-user mt-2 landing-register mt-auto" :disabled="(invalidName && invalidFamily)" id="tor-register">مرحله بعد<span v-if="loading" class="spinner-border text-light spinner-border-sm" role="status" aria-hidden="true" style="display: inline-block;margin-right: 5px;"></span>
                      <i v-else class="ic-arrow-right"></i>
                    </button>
                  </form>
                </div>
              </transition>

            </div>

            <!-- STEP 4 -->
            <div class="inner" v-if="step==3" :key="step">
              <transition name="fade" mode="out-in" tag="div">
                <div class="inner" v-if="childStep==1" :key="childStep">
                  <header class="header-title">
                    <h2>انتخاب نوع رژیم</h2>
                  </header>
                  <div class="pay-help-bmi">
                    <p class="color-black text-right mb-0 sans">با دریافت رژیم برنافیت، عضو ماراتن لاغری گروهی می‌شی و از هم‌گروهیات انگیزه می‌گیری. با توجه به شرایط جسمانیت، برنافیت رژیم‌های زیر رو بهت پیشنهاد می‌ده:</p>
                    <p class="color-black text-right mb-2 sans"></p>
                    <p class="color-black text-right mb-0 bold-text sans">یکی از رژیم‌های زیر رو انتخاب کن:</p>
                  </div>
                  <section id="product-box" class="mb-20">
                    <div v-for="package in packages.products" :key="package.id" v-if="package.status === true">
                      <input type="radio" :id="'control_0'+package.id" name="select" v-model="form.packageSelectId" :value="package.id">
                      <label :for="'control_0'+package.id">
                        <div class="title-box">
                          <h2 class="h6">{{package.title}}</h2>
                          <p class="mb-0 h6 font-weight-normal" v-for="description in package.description">{{description}}</p>
                        </div>
                        <div class="amount-box">
                          <div class="amount-content" v-if="package.price.offer">
                            <p>{{package.price.amount}}</p>
                            <p>
                              <span>{{Math.round(((package.price.amount - (package.price.amount - package.price.offer))/package.price.amount)*100)}}</span>
                              <span>%</span>
                            </p>
                          </div>
                          <div class="amount-off">
                            <p>{{(package.price.amount - package.price.offer) === 0 ? "رایگان" : (package.price.amount - package.price.offer)}}</p>
                            <p>هزارتومان</p>
                          </div>
                        </div>
                      </label>
                    </div>
                    <p class="error mt-1 mb-0 sans" v-if="!form.packageSelectId">لطفا محصول مورد نظر خودرا انتخاب کنید</p>
                  </section>
                  <div class="button_product_sticky">
                    <div class="amount">
                      <p class="bold-text">مجموع:</p>
                      <p style="color: #002c00;" class="bold-text">{{this.returnSumPrice.selectedPackageItemSum}} <span class="h6">تومان</span></p>
                    </div>
                    <button type="submit" @click="errorHandle(1)" class="btn-landing landing-selectdiet m-0" id="btn-register-pay">
                      {{this.form.payment_method === 'card-payment' ? 'ارسال مشخصات' : 'خرید و دریافت رژیم'}}
                      <span v-if="loading" class="spinner-border text-light spinner-border-sm" role="status" aria-hidden="true"></span>
                      <i v-else class="ic-arrow-right"></i>
                    </button>
                  </div>
                </div>
                <div class="inner" v-if="childStep==2" :key="childStep">
                  <header class="header-title">
                    <h2>انتخاب نوع پشتیبانی</h2>
                  </header>
                  <div class="pay-help-bmi">
                    <p class="color-black text-right mb-2 sans">کسایی که از برنافیت رژیم گرفتن، پشتیبان رو به عنوان مهم‌ترین عامل موفقیت‌شون معرفی می‌کنن. پشتیبان در طول رژیم همراهته، به همه سوالات پاسخ میده و نمی‌ذاره انگیزه‌ت کم شه.</p>
                    <p class="color-black text-right mb-0 bold-text sans">یکی از روش‌های پشتیبانی زیر رو انتخاب کن:</p>
                  </div>
                  <section id="product-box" class="mb-20">
                    <div v-for="package in selectedPackageItem.option.support" :key="package.id">
                      <input type="radio" :id="'control_0'+package.id" name="select" v-model="packagesSupportSelectId" :value="package.id">
                      <label :for="'control_0'+package.id">
                        <div class="title-box">
                          <h2 class="h6">{{package.title}}</h2>
                          <p class="mb-0 h6 font-weight-normal" v-for="description in package.description">{{description}}</p>
                        </div>
                        <div class="amount-box">
                          <div class="amount-content" v-if="package.price.offer">
                            <p>{{package.price.amount}}</p>
                            <p>
                              <span>{{Math.round(((package.price.amount - (package.price.amount - package.price.offer))/package.price.amount)*100)}}</span>
                              <span>%</span>
                            </p>
                          </div>
                          <div class="amount-off">
                            <p>{{(package.price.amount - package.price.offer) === 0 ? "رایگان" : (package.price.amount - package.price.offer)}}</p>
                            <p>هزارتومان</p>
                          </div>
                        </div>
                      </label>
                    </div>
                    <p class="error mt-1 mb-0" v-if="!form.packageSelectId">لطفا محصول مورد نظر خودرا انتخاب کنید</p>
                  </section>
                  <div class="button_product_sticky">
                    <div class="amount">
                      <p class="bold-text">مجموع:</p>
                      <p style="color: #002c00;" class="bold-text">{{this.returnSumPrice.selectedPackageItemSum}} <span class="h6">تومان</span></p>
                    </div>
                    <button type="submit" @click="errorHandle(2)" class="btn-landing landing-selectsupport m-0" id="btn-register-pay">
                      {{this.form.payment_method === 'card-payment' ? 'ارسال مشخصات' : 'خرید و دریافت رژیم'}}
                      <span v-if="loading" class="spinner-border text-light spinner-border-sm" role="status" aria-hidden="true"></span>
                      <i v-else class="ic-arrow-right"></i>
                    </button>
                  </div>
                </div>
                <div class="inner" v-if="childStep==3" :key="childStep">
                  <header class="header-title">
                    <h2>انتخاب برنامه ورزشی</h2>
                  </header>
                  <div class="pay-help-bmi">
                    <p class="color-black text-right mb-2 sans">اگه می‌خوای با سرعت بیشتری چربی‌های شکم و پهلوت رو آب کنی، می‌تونی در کنار رژیمت برنامه ورزشی هم داشته باشی.</p>
                    <p class="color-black text-right mb-0 bold-text sans">برای انتخاب برنامه ورزشی، کلیک کن:</p>
                  </div>
                  <section id="product-box" class="mb-20">
                    <div v-for="package in selectedPackageItem.option.exercise" :key="package.id">
                      <input type="checkbox" :id="'control_0'+package.id" name="select" v-model="packagesSportsSelectId" :value="package.id" class="product_input">
                      <label :for="'control_0'+package.id">
                        <div class="title-box">
                          <h2 class="h6">{{package.title}}</h2>
                          <p class="mb-0 h6 font-weight-normal" v-for="description in package.description">{{description}}</p>
                        </div>
                        <div class="amount-box">
                          <div class="amount-content" v-if="package.price.offer">
                            <p>{{package.price.amount}}</p>
                            <p>
                              <span>{{Math.round(((package.price.amount - (package.price.amount - package.price.offer))/package.price.amount)*100)}}</span>
                              <span>%</span>
                            </p>
                          </div>
                          <div class="amount-off">
                            <p>{{(package.price.amount - package.price.offer) === 0 ? "رایگان" : (package.price.amount - package.price.offer)}}</p>
                            <p>هزارتومان</p>
                          </div>
                        </div>
